{
    "family": "hello-world-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::882424314092:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "hello-world-container",
        "image": "882424314092.dkr.ecr.us-west-1.amazonaws.com/hello-world-server:latest",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 3000,
            "protocol": "tcp"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/hello-world",
            "awslogs-region": "us-west-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ]
  }